<h1>
  Edit like #<%= @like.id %>
</h1>

<hr>

<form action="/update_like/<%= @like.id %>" method="post">

  <div class="form-group">
    <label for="user_id">
      User
    </label>

    <input type="text" id="user_id" name="user_id" class="form-control" value="<%= @like.user_id %>">
  </div>

  <div class="form-group">
    <label for="photo_id">
      Photo
    </label>

    <input type="text" id="photo_id" name="photo_id" class="form-control" value="<%= @like.photo_id %>">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Update like
  </button>
</form>

<hr>

<a href="/likes">
  Go back
</a>
